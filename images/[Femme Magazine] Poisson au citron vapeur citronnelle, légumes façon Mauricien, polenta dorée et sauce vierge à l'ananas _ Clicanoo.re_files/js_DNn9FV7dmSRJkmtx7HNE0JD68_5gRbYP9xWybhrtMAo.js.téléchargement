function uneImg(){
    (function($) {
        var d = new Date();
        var day = ("0" + d.getDate()).slice(-2);
        var month = ("0" + (d.getMonth() + 1)).slice(-2);
        var year = d.getFullYear();
        var filename="une-"+year+"-"+month+"-"+day+".jpg";
        var dataurl='https://www.clicanoo.re/sites/default/files/vignette_mag/'+filename;
        $('#une').attr('data-original',dataurl);
        $('#uneside').attr('src',dataurl);

        $('#panelMenu #uneside').load(function(){
                var uneHeight=$('#uneside').height();
                var editionHeight=$('.abonnement-edition-wrapper').height();
                var paddingTopEdition=uneHeight-editionHeight;
                console.log('Une:'+uneHeight);
                $('.abonnement-date').css('padding-top',paddingTopEdition);
         });
    })(jQuery);
}



function editionDate(){
    (function($) {
        var d = new Date();

        var day = ("0" + d.getDate()).slice(-2);
        var year = d.getFullYear();
        var month = new Array();
        month[0] = "Janvier";
        month[1] = "Fvrier";
        month[2] = "Mars";
        month[3] = "Avril";
        month[4] = "Mai";
        month[5] = "Juin";
        month[6] = "Juillet";
        month[7] = "Aot";
        month[8] = "Septembre";
        month[9] = "Octobre";
        month[10] = "Novembre";
        month[11] = "Dcembre";
        var monthname = month[d.getMonth()];

        var edition=day+" "+monthname+" "+year;
        $('.editiondate').html(edition);
    })(jQuery);
}

function backgroundColor(){
    (function($) {
        $('body').attr('style','background-color:#FFFF !important;background-position:center 180px !important;');
    })(jQuery);
}

function sortNumber(a,b)    {
    return a - b;
}

function adjustHeight(){
    (function($) {
        var heightsEmploi = new Array();
        $('#block-frontemplois .frontemploi-item .frontemploi-inner').each(function(){
            $(this).css('height', 'auto');
            heightsEmploi.push($(this).outerHeight());
        });
        heightsEmploi = heightsEmploi.sort(sortNumber).reverse();
        $('#block-frontemplois .frontemploi-item .frontemploi-inner').css('height', heightsEmploi[0]);


        var heightsAnnonces = new Array();
        $('#block-frontannonces .frontannonces-item .frontannonces-inner').each(function(){
            $(this).css('height', 'auto');
            heightsAnnonces.push($(this).outerHeight());
        });
        heightsAnnonces = heightsAnnonces.sort(sortNumber).reverse();
        $('#block-frontannonces .frontannonces-item .frontannonces-inner').css('height', heightsAnnonces[0]);

        var heightsInfluenceurs = new Array();
        $('.view-bloc-auteur-blogeur .views-row').each(function(){
            $(this).css('min-height', '220px');
            heightsInfluenceurs.push($(this).outerHeight());
        });
        heightsInfluenceurs = heightsInfluenceurs.sort(sortNumber).reverse();
        $('.view-bloc-auteur-blogeur .views-row').css('height', heightsInfluenceurs[0]);
    })(jQuery);
}


function adjustHeightSide(){
    (function($) {
        var sidebarHeight=$('#sidebar-second').height();
        var sidebarHeightSumo=sidebarHeight+900;
        var contentHeight=$('#content').height();
        var contentTitleHeight=$('#content_title').height();
        var fullcontentHeight=contentHeight+contentTitleHeight+350;

        if(sidebarHeightSumo > fullcontentHeight){
            var diffContentHeight=sidebarHeightSumo - fullcontentHeight;
            var finalContentHeight=contentHeight+diffContentHeight;
            $('#content').height(finalContentHeight);
        }
    })(jQuery);
}



function randomblock() {
    (function($) {
        var cards = $("#block-partenaires .item-part");
        for (var i = 0; i < cards.length; i++) {
            var target = Math.floor(Math.random() * cards.length - 1) + 1;
            var target2 = Math.floor(Math.random() * cards.length - 1) + 1;
            cards.eq(target).before(cards.eq(target2));
        }

        var cards2 = $("#block-partenaires .part-head-item-link");
        for (var i = 0; i < cards2.length; i++) {
            var target_1 = Math.floor(Math.random() * cards2.length - 1) + 1;
            var target2_2 = Math.floor(Math.random() * cards2.length - 1) + 1;
            cards2.eq(target_1).before(cards2.eq(target2_2));
        }
    })(jQuery);
}

function backPartImg() {
    (function($) {
        $("#block-partenaires .item-part .part-img").each(function() {
            var imgSrc = $(this).find('img').attr('data-original');
            var imgCss = 'url(' + imgSrc + ')';
            $(this).css('background-image', imgCss);
        });
    })(jQuery);
}

function slickInit() {
    (function($) {
        $('#block-partenaires .items-wrapper').slick({
            infinite: true,
            autoplay: true,
            autoplaySpeed: 5000,
            dots: true,
            arrows: false,
        });
        initLazyLoad();
        //backPartImg();
    })(jQuery);
}

function slickInitPleinlesyeux(){
    (function($) {
        $('#block-views-block-bloc-plein-les-yeux-block-2 .view-content').slick({
            infinite: true,
            slidesToShow: 2,
            slidesToScroll: 2,
            autoplay: false,
            autoplaySpeed: 10000,
            dots: true,
            arrows: false
        });
    })(jQuery);
}


function partenaireContent(){
    (function($) {
        var ver = (Math.random()*49+1).toFixed(3);

        $.getJSON('https://partenaires.clicanoo.re/application/json/blockpart2.json?v=' + ver + '', function(data) {
            $.each(data, function(idx, obj) {
                for (i = 0; i < obj.length; i++) {
                    var title = obj[i]['title'];
                    var image = obj[i]['image'];
                    var url = obj[i]['url'];
                    var summary = obj[i]['summary'];
                    var siteurl = obj[i]['siteurl'];
                    var blogname = obj[i]['blogname'];
                    var icon = obj[i]['icon'];
                    var html = '<div class=\"item-part\"><a href=\"' + siteurl + '\" target=\"_blank\"><div class=\"part-header\"><img class="lazy" src=\"\" data-original=\"' + icon + '\"><span>' + blogname + '</span></div></a><a href=\"' + url + '\" target=\"_blank\"><div class=\"part-wrap\"><div class=\"part-img\"><img class="lazy" src=\"\" data-original=\"' + image + '\"></div><div class=\"part-title\"><h4>' + title + '</h4><div class=\"part-text\">' + summary + '</div></div></div></a></div>';
                    $('.items-wrapper').append(html);
                    var htmlpart = '<a class="part-head-item-link" href=\"' + siteurl + '\" target=\"_blank\"><div class=\"part-head-item\"><img class="lazy"  src=\"\" data-original=\"' + icon + '\"><span>' + blogname + '</span></div></a>';
                    $('.part-head').append(htmlpart);
                }

            });
            randomblock();
            slickInit();
            //backPartImg();
            initLazyLoad();
        });

        console.log(ver);

    })(jQuery);
}

function adsInMobileArticle(){
    (function($) {
        var userAgent = navigator.userAgent || navigator.vendor || window.opera;
        var monDevice = new MobileDetect(userAgent);
        if( monDevice.phone()){
            var adtags1='<div id="sas_59395"></div><script type="application/javascript">sas.cmd.push(function(){sas.call("std", {siteId: 173580, pageId: 851808,formatId: 59395,target: ""});});</script><noscript><a href="//www3.smartadserver.com/ac?jump=1&nwid=1544&siteid=173580&pgname=clicanoo_mobile&fmtid=59395&visit=m&tmstp=[timestamp]&out=nonrich" target="_blank"><img src="//www3.smartadserver.com/ac?out=nonrich&nwid=1544&siteid=173580&pgname=clicanoo_mobile&fmtid=59395&visit=m&tmstp=[timestamp]" border="0" alt="" /></a></noscript>';
            $('.node__content .field--name-body > p:nth-of-type(1)').after(adtags1);
            var adtags2='<div id="sas_59398"></div><script type="application/javascript">sas.cmd.push(function(){sas.call("std", {siteId: 173580,pageId: 851808,formatId: 59398,target: ""});});</script><noscript><a href="//www3.smartadserver.com/ac?jump=1&nwid=1544&siteid=173580&pgname=clicanoo_mobile&fmtid=59398&visit=m&tmstp=[timestamp]&out=nonrich" target="_blank"><img src="//www3.smartadserver.com/ac?out=nonrich&nwid=1544&siteid=173580&pgname=clicanoo_mobile&fmtid=59398&visit=m&tmstp=[timestamp]" border="0" alt="" /></a></noscript>';
            $('.node__content .field--name-body > p:nth-of-type(6)').after(adtags2);
        }
    })(jQuery);
}

