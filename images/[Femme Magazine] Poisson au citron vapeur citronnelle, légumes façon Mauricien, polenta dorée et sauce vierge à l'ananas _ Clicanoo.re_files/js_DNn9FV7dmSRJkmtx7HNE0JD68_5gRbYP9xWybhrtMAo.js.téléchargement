//     $no_refresh_block = $("div#no-refresh-page");
//     // console.log($no_refresh_block);
//     if($no_refresh_block.length > 0){
//         clicanoo.autoRefresh = false;
//     }else{
//         clicanoo.autoRefresh = true;
//     }
// }

clicanoo._log = function(string){
    if (clicanoo.showLog) {
        console.log(string);
    }
};


(function($) {
    $(document).ready(function() {
        clicanoo.centerElement(".site-logo.lien-menu", ".menu .menu-center");
        clicanoo.centerElement("#nav-main>.nav", "#main-menu .container");
        uneImg();
        domainImg();
        ultimediaVideoImageHide();
        taboola();
        vignettekiosque();
        downPDF();
        initCoverflow();
        initLazyLoad();
        panelToggle();
        editionDate();
        menulistToggle();
        backgroundColor();
        adjustHeight();
        partenaireContent();
        lightSliderInit();
        imgCaption();
        adjustHeightSide();
        userLoginFormPlaceholder();
        slickInitPleinlesyeux();
        annnoncesHeight();
        adjustHeight();
        adsInMobileArticle();
        aboContactModal();
        blogType();
        initLazyLoadBackgroundImg();
        refreshPage();
        cookiebutton();
        carteElection();
        candidatHover();




            $(window).resize(function () {
            clicanoo.centerElement(".site-logo.lien-menu", ".menu .menu-center");
            clicanoo.centerElement("#nav-main>.nav", "#main-menu .container");
        });
        clicanoo.showSearch();
    });

    $(window).resize(function(){
        domainImg();
        adjustHeight();
        annnoncesHeight();
    });
    $(window).scroll(function(){
        backPartImg();
        adjustHeight();
    });
    $(window).ready(function(){

        // clicanoo.autoRefreshActive();
        //
        // if(clicanoo.autoRefresh){
        //     refreshPage();
        // }


        clicanoo.adaptToPubHabillage = function(){
            var windowWidth = $(window).width();
            setTimeout(function(){
                if(windowWidth > 753){
                    clicanoo._log('modif css body');
                    $('body').attr('style',$('body').attr('style')+' background-position: center 211px !important;');
                    clicanoo._log($('.main').css('margin-top'));
                    if($('.main').css('margin-top') != ''){
                        if($('.main').css('margin-top') != '0px'){
                            if($('.main').css('margin-top') != 'auto'){
                                $('.main').attr('style','margin-top: 211px !important;');
                            }
                        }
                    }
                }
            },5000);
        }
    });

    $(window).load(function(){
        digiteka();
    });
}(jQuery));


Drupal.behaviors.myBehavior = {
    attach: function (context, settings) {
        domainImg();
        adjustHeightSide();
        adjustHeight();
        initLazyLoad();
        Liveload();
        LiveRrefreshbutton();
        LiveRefresh();
    }
};

