(function($) {
        var $videoDiv = $('.field--name-field-video-embed');

        if ( $videoDiv.length){
            $('.lSSlideWrapper').hide();
        }
    })(jQuery);
}

function taboola(){
    (function($) {
        window._taboola = window._taboola || [];
        _taboola.push({flush: true});
    })(jQuery);
}


function initCoverflow(){
    (function($) {
        $('#block-views-block-bloc-plein-les-yeux-block-1 .view-content').coverflow({
            active: 2,
            angle:0,
            select: function(event, ui){
            }
        });

        $('#block-views-block-bloc-plein-les-yeux-block-1 .view-content img').click(function() {
            if( ! $(this).hasClass('ui-state-active')){
                return;
            }

            $('#block-views-block-bloc-plein-les-yeux-block-1').coverflow('next');
        });
    })(jQuery);
}

function panelToggle(){
    (function($) {
        $('#menuToggle').click(function() {
            $('#panelMenu').toggleClass('active');
            $('.backdrop').toggleClass('hidden');
        });
        $('#panelMenu .header-left').click(function() {
            $('#panelMenu').toggleClass('active');
            $('.backdrop').toggleClass('hidden');
        });
        $('.backdrop').click(function() {
            $('#panelMenu').toggleClass('active');
            $('.backdrop').toggleClass('hidden');
        });
    })(jQuery);
}

function menulistToggle(){
    (function($) {
        $('.menuhead i.fa-plus').click(function() {
            $(this).parent().parent().find('ul.second').toggleClass('hidden');
            $(this).toggleClass('hidden');
            $(this).parent().find('i.fa-times').toggleClass('hidden');
        });
        $('.menuhead i.fa-times').click(function() {
            $(this).parent().parent().find('ul.second').toggleClass('hidden');
            $(this).toggleClass('hidden');
            $(this).parent().find('i.fa-plus').toggleClass('hidden');
        });
    })(jQuery);
}


function userLoginFormPlaceholder(){
    (function($) {
        $('#user-login-form .js-form-item').each(function( index ) {
            var description=jQuery(this).find('.description').text();
            description = description.replace(/ +(?= )/g,'');
            $(this).find('.description').hide();
            $(this).find('input').attr('placeholder',description)
        });
        $('#user-pass .js-form-item').each(function( index ) {
            var description=jQuery(this).find('.description').text();
            description = description.replace(/ +(?= )/g,'');
            $(this).find('.description').hide();
            $(this).find('input').attr('placeholder',description)
        });
        $('#cas-server-user-login .js-form-item').each(function( index ) {
            var description=jQuery(this).find('.description').text();
            description = description.replace(/ +(?= )/g,'');
            $(this).find('.description').hide();
            $(this).find('input').attr('placeholder',description)
        });



            $('#user-register-form #edit-mail').attr('placeholder','Saisissez votre adresse email');
            $('#user-register-form #edit-pass-pass1').attr('placeholder','Crez votre mot de passe');
            $('#user-register-form #edit-pass-pass2').attr('placeholder','Confirmez votre mot de passe');
            $('#edit-field-blog-themes-0-value').attr('placeholder','#Thmatique');
    })(jQuery);
}


