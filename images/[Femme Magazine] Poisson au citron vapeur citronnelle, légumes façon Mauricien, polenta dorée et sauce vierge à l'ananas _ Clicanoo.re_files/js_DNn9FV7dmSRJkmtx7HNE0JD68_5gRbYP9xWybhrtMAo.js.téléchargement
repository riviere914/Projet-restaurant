function LiveRrefreshbutton(){
    (function($) {
        $('.field--name-field-live').after('<div id="live"></div>');
        var liveWrapper='<div id="live-refresh"><div class="refresh-button"><i class="fa fa-refresh"></i> Actualiser</div></div>';
        $('#live').append(liveWrapper);
    })(jQuery);
}

function Liveload(){
    (function($) {
        $('.field--name-field-live').hide();
        var ver = (Math.random()*49+1).toFixed(3);
        var baseurl = window.location.origin;
        var liveId= $('.field--name-field-live').text();
        console.debug(liveId);
        if(liveId !== ''){
            var jsonUrl= baseurl+'/api/live/' + liveId + '?ver='+ver;
            $.getJSON(jsonUrl, function(data) {
                $.each(data, function(index) {
                    var title = data[index].title;
                    var body = data[index].body;
                    var created = data[index].created;
                    var html = '<div class="live-item"><div class="live-created">'+created+'</div><h3 class="livetitle">'+title+'</h3><div class="live-body">'+body+'</div></div>';
                    $('#live-refresh').after(html);
                });
                $('.live-item').wrapAll('<div class="live-wrapper">');
                initLazyLoad();
                adjustHeightSide();
                $.getScript("https://platform.twitter.com/widgets.js", function( data, textStatus, jqxhr ) {
                });
                $("#live-refresh i").removeClass('fa-spin');
            });
        }
    })(jQuery);
}

function LiveRefresh(){
    (function($) {
        $( "#live-refresh" ).click(function() {
            $(".live-wrapper").remove();
            $("#live-refresh i").addClass('fa-spin');
            Liveload();
        });
    })(jQuery);
}


function carteElection(){
    (function($) {
        $( ".carte_content polygon" ).hover(
            function() {
                $( this ).toggleClass('hover');
            }
        )

        var url  = $(location).attr('pathname');
        var result = url.split('/');
        var tid = result[3];
        var selector = '.view-bloc-maire-sortant svg #'+tid+'';
        var selected = $(selector).addClass('active');

        $(selector).css('fill','#e2001a');

        $( ".results_bar" ).each(function() {
          var score = $( this ).find('.candidat_score').text();
          $( this ).find('.bar_inner').css('width',score);
        });

    })(jQuery);
}

function candidatHover(){
    jQuery('.maire .maireRow').on( 'click', function() {
        jQuery('.maireModal').hide();
        jQuery(this).parent().find('.maireModal').toggle();
    });

    jQuery('.maireModalHeader').on( 'click', function() {
        jQuery('.maireModal').hide();
    });

}


var clicanoo = window.clicanoo || {};

clicanoo = {
    showLog:false,
    showSearchBar:true,
    autoRefresh:true
}


clicanoo.centerElement = function(element, container) {

        clicanoo._log("center element: "+element);
        var totalWidth = $(container).width();
        var widthElement = $(element).width();
        var marginContainer = parseInt($(container).css("marginLeft").replace("px", ""));
        var paddingContainer = parseInt($(container).css("paddingLeft").replace("px", ""));
        var calTotal = (totalWidth - marginContainer - paddingContainer);
        var elementPercent = 0;
        if (calTotal > widthElement) {
            elementPercent = (widthElement * 100)/ (totalWidth - marginContainer - paddingContainer);
        }else{
            elementPercent = (widthElement * 100)/ totalWidth;
        }
        var marginLeftRight = (100 - elementPercent) / 2;

        clicanoo._log("totalWidth: "+totalWidth);
        clicanoo._log("calTotal: "+calTotal);
        clicanoo._log("marginContainer: "+marginContainer);
        clicanoo._log("paddingContainer: "+paddingContainer);
        clicanoo._log("widthElem: "+widthElement);
        clicanoo._log("elementPercent: "+elementPercent);
        clicanoo._log("margin: "+marginLeftRight);
        if (marginLeftRight < 0) {
            $(element).css({marginLeft:0+"%"});
        }else{
            $(element).css({marginLeft:marginLeftRight+"%"});
        }
};

clicanoo.showSearch = function(){
    var searchBarBtn = $(".menu #search-btn");
    var searchBar = $("#formSearch");
    if (clicanoo.showSearchBar) {
        searchBarBtn.click(function(){
            if(searchBar.hasClass("active")){
                searchBarBtn.removeClass("active");
                searchBar.removeClass("active");
            }else{
                searchBarBtn.addClass("active");
                searchBar.addClass("active");
            }
        });
    } else {
        searchBarBtn.attr("href" , searchBarBtn.attr("data-href"));
    }
}

