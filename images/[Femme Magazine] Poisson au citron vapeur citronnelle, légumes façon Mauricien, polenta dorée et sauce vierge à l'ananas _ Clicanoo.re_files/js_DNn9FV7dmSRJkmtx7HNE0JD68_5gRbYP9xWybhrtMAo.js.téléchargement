function blogType(){
    (function($) {
        $('.view-bloc-auteur-blogeur .views-row').each(function(){
            var type=$(this).find('.views-field-field-type-blogueur').text();
            $(this).addClass(type);
        });
        $('.view-page-auteur-blogeur .views-row').each(function(){
            var type=$(this).find('.views-field-field-type-blogueur').text();
            $(this).addClass(type);
        });
    })(jQuery);
}


