$(document).ready(function() {
    // console.log("--------block resize log-------");
    // console.log("--------block resize log init-------");
    var is_chrome = navigator.userAgent.indexOf('Chrome') > -1;
    var is_explorer = navigator.userAgent.indexOf('MSIE') > -1;
    var is_firefox = navigator.userAgent.indexOf('Firefox') > -1;
    var is_safari = navigator.userAgent.indexOf("Safari") > -1;
    var is_opera = navigator.userAgent.toLowerCase().indexOf("op") > -1;
    if ((is_chrome)&&(is_safari)) {is_safari=false;}
    if ((is_chrome)&&(is_opera)) {is_chrome=false;}
    // alert("safari = "+is_safari);
    // console.log("is_chrome = "+is_chrome);
    // console.log("is_explorer = "+is_explorer);
    // console.log("is_safari = "+is_safari);
    // console.log("is_opera = "+is_opera);
    // console.log("is_firefox = "+is_firefox);

    // menu dropdown if safari navigator
    if (is_safari != false){
        // console.log("menu en dropdown");
        // console.log("is_safari = "+is_safari);
        // recuperer menu
        var hideTime;
        var menuMainElem       = document.getElementById("block-jir-main-menu");
        var btnMenuMagazines   = $("#btn-magazine");
        var btnMenuCommunes    = $("#btn-region");
        var btnMenuServices    = $("#btn-services");
        var menuMagazines      = $("#menu-popover-content-magazines");
        var menuCommunes       = $("#menu-popover-content-communes");
        var menuServices       = $("#menu-popover-content-services");

        btnMenuMagazines.on("click", function(){ 
            hideMenu();
            menuMainElem.style.overflow = 'unset';
            btnMenuMagazines.attr("Class", "active-menu");
            menuMagazines.attr("Class", "show-menu");
            hideTime = setTimeout(hideMenu, 5000);
            menuMagazines.mouseenter(function(){
               clearTimeout(hideTime); 
            }).mouseleave(function(){
                hideTime = setTimeout(hideMenu, 2000);
            });
        });
        btnMenuCommunes.on("click", function(){ 
            hideMenu();
            menuMainElem.style.overflow = 'unset';
            btnMenuCommunes.attr("Class", "active-menu");
            menuCommunes.attr("Class", "show-menu");  
            hideTime = setTimeout(hideMenu, 5000);
            menuCommunes.mouseenter(function(){
               clearTimeout(hideTime); 
            }).mouseleave(function(){
                hideTime = setTimeout(hideMenu, 2000);
            });

        });
        btnMenuServices.on("click", function(){ 
            hideMenu(menuMainElem);
            menuMainElem.style.overflow = 'unset';
            btnMenuServices.attr("Class", "active-menu");
            menuServices.attr("Class", "show-menu");
            hideTime = setTimeout(hideMenu, 5000); 
            menuServices.mouseenter(function(){
               clearTimeout(hideTime); 
            }).mouseleave(function(){
                hideTime = setTimeout(hideMenu, 2000);
            });

        });

    }
    
    
    if($('.fa.fa-print>a').length > 0){$articleTitle = $('#block-jir-page-title h1').html();$('.fa.fa-print>a').attr('href','#'); $('.fa.fa-print>a').attr('onclick',"imprimer_bloc(\""+$articleTitle+"\")" );}
   

    //---------------initialisation
    
    // $('#navigation-account .lien-mon-compte').mouseenter(function(){
    //   alert('hover');
    //   $(this).next('b').click();
    // });
    
    var buttonSearch = $('#searchBarButton');
    var barSearch = $('#block-jir-search');
    var barSearchInput = $('#block-jir-search #edit-keys');
    // var barSearchSubmit = $('#block-jir-search #edit-actions');
    barSearch.hide();
    if($("#block-views-block-dossier-small-wrapper").length){ setTimeout(resizeDossierSmall, 10000);}
    // if($('#block-jir-views-block-content-recent-block-1').length){ setTimeout(resizeEdito, 6000);}
    // if($("#block-views-block-wrapper-rubrique-block-1").length){ setTimeout(resizeRubriqueMedium1, 15000);}
    // if($("#block-views-block-rubrique-block-18").length){ setTimeout(resizeRubriqueMedium2, 12000);}
    setTimeout(resizeRubriqueMedium1, 11000);
    setTimeout(resizeRubriqueMedium2, 10000);

    //----------------event
    // console.log("--------block resize log event-------");
    buttonSearch.click(function() { showSearchBar();});
    barSearch.focusout(function() { setTimeout(hideSearchBar,2000);});
    // barSearchSubmit.click(function() { showSearchBar();});
    
    /**
     * set Margin Top #block-jir-views-block-content-recent-block-1
     * @param  {int} $('#block-jir-views-block-content-recent-block-1').length  width element if exist
     * 
     */
    // if($('#block-views-block-edito-block-1').length){
    //     new ResizeSensor(jQuery('#block-pub-sumo'), function(){ 
    //         // console.log('main content dimension changed');
    //         setTimeout(resizeEdito, 1000);
    //     });
