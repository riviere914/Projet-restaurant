        $("#block-sideannonces-2").lazyload({
            effect: "fadeIn",
            threshold: 200,
            skip_invisible : false
        });
        $('#block-frontannonces').attr('data-original','/themes/jir/assets/img/backannoncefront.png');
        $("#block-frontannonces").lazyload({
            effect: "fadeIn",
            threshold: 200,
            skip_invisible : false
        });
        $('.footer-abonnement .abo-left').attr('data-original','/themes/jir/assets/img/footer-img.png');
        $(".footer-abonnement .abo-left").lazyload({
            effect: "fadeIn",
            threshold: 200,
            skip_invisible : false
        });
        $('#abofront .frontAboRight').attr('data-original','/themes/jir/assets/img/footer-img.png');
        $("#abofront .frontAboRight").lazyload({
            effect: "fadeIn",
            threshold: 200,
            skip_invisible : false
        });
        $('#block-views-block-bloc-choix-de-la-redac-block-1').attr('data-original','/themes/jir/assets/img/backredac.jpg');
        $("#block-views-block-bloc-choix-de-la-redac-block-1").lazyload({
            effect: "fadeIn",
            threshold: 200,
            skip_invisible : false
        });



    })(jQuery);
}


