    //----------------function
    /**
    *Show search barre
    */
    function showSearchBar(){
        barSearch.toggle("slow");
        barSearchInput.focus();
    }
    /**
    *Hide search barre
    */
    function hideSearchBar(){  barSearch.toggle("slow"); }

    /**
    *Resize Dossier Small
    *
    * @return {[type]}
    */
    function resizeDossierSmall() {
        $dossierSmall=[];
        $article=[];
        $wrapperDossierSmall = new Block($("#block-views-block-dossier-small-wrapper"));
        $dossierSmall[0] = new Block($("#block-views-block-dossier-small-wrapper .article-dossier-small:eq(0)"));
        $dossierSmall[1] = new Block($("#block-views-block-dossier-small-wrapper .article-dossier-small:eq(1)"));
        $dossierSmall[2] = new Block($("#block-views-block-dossier-small-wrapper .article-dossier-small:eq(2)"));
        $dosPG = getPlusGrand($dossierSmall[0], $dossierSmall[1], $dossierSmall[2]);
        // alert($dosPG);
        $dossier = $dossierSmall[$dosPG].getBlock();

        $article[0] = new Block_sp($dossier.children(".views-row.article-dossier:eq(0)"));
        $article[1] = new Block_sp($dossier.children(".views-row.article-dossier:eq(1)"));
        $article[2] = new Block_sp($dossier.children(".views-row.article-dossier:eq(2)"));
        $artPG = getPlusGrand($article[0], $article[1], $article[2]);
        // alert($artPG);
        $articlePG = new Block($("#block-views-block-dossier-small-wrapper .article-dossier-small:eq("+$dosPG+") .views-row.article-dossier:eq("+$artPG+")"));
        $articleHeight = $articlePG.getHeight();
        // alert($articleHeight);
        var nbArticleResize = 0
        $articleRZ=[];
        for (var i = 0; i < 3; i++) {
            for (var j = 0; j < 3; j++) {
                $articleRZ[nbArticleResize] = new Block($("#block-views-block-dossier-small-wrapper .article-dossier-small:eq("+i+") .views-row.article-dossier:eq("+j+")"));
                $articleRZ[nbArticleResize].setHeight($articleHeight+20);
                nbArticleResize++;
            }
        }
        

    }
    /**
     * @param  {Object}
     * @param  {Object}
     * @param  {Object}
     * @return {int}
     */
    function getPlusGrand($block1, $block2, $block3) {
        $modelHeight = [];
        if ($block1.getHeight() > $block2.getHeight()) {$modelHeight[1] = 1}else{$modelHeight[1] = 2}
        if ($block1.getHeight() > $block3.getHeight()) {$modelHeight[2] = 1}else{$modelHeight[2] = 3}
        if ($block2.getHeight() > $block3.getHeight()) {$modelHeight[3] = 2}else{$modelHeight[3] = 3}
        $calc  =  $modelHeight[1]+$modelHeight[2]+$modelHeight[3];  
        if($calc == 4){ $plusGrand = 1 }
        if($calc == 5){ if ($modelHeight[1] == 1) {$plusGrand = 1} else {$plusGrand = 2}}   
        if($calc == 7){ $plusGrand = 3 }
        if($plusGrand == 1){ return 0; }
        if($plusGrand == 2){ return 1; }
        if($plusGrand == 3){ return 2; }
    }


    /**
     * @return {[type]}
     */
    function resizeRubriqueMedium1() {
        // console.log("test function resizeRubriqueMedium1()");
        if ($("#block-views-block-wrapper-rubrique-block-1").length) {

            $wrapperBlockEcoSport   = new Block($("#block-views-block-wrapper-rubrique-block-1"));
            $wrapperBlockSport      = new Block($("#wrapper-sport"));
            $blockSport1            = new Block($("#wrapper-sport>.wrapper:eq(0)"));
            $blockSport2            = new Block($("#wrapper-sport>.wrapper:eq(1)"));
            $blockSportLink         = new Block($("#wrapper-sport .view-footer"));
            $wrapperBlockEco        = new Block($("#wrapper-economie"));
            $blockEco1              = new Block($("#wrapper-economie>.wrapper:eq(0)"));
            $blockEco2              = new Block($("#wrapper-economie>.wrapper:eq(1)"));
            $blockEcoLink           = new Block($("#wrapper-economie .view-footer"));
            $calcWrapper = $wrapperBlockEco.getHeight() - $wrapperBlockSport.getHeight();
            // $calcBlockEco = $blockEco1.getHeight()+"/"+$blockEco2.getHeight();
            // $calcBlockSport = $blockSport1.getHeight()+"/"+$blockSport2.getHeight();

            if ($calcWrapper < 0) {
                // set height blockWrapperEco
                $wrapperBlockEco.setHeight($wrapperBlockEco.getHeight()+$calcWrapper*-1);
                $wrapperBlockEco        = new Block($("#wrapper-economie"));
                $diff = $wrapperBlockEco.getHeight() - ($blockEco1.getHeight() + $blockEco2.getHeight() + $blockEcoLink.getHeight());
                // set height blockEco2
                $blockEco2 = new Block($("#wrapper-economie>.wrapper:eq(1) .view-content"));
                $blockEco2.setHeight($blockEco2.getHeight()+$diff+$blockEcoLink.getHeight());
            }else{
                // set height blockWrapperSport
                $wrapperBlockSport.setHeight($wrapperBlockSport.getHeight()+$calcWrapper);
                $wrapperBlockSport      = new Block($("#wrapper-sport"));
                $diff = $wrapperBlockSport.getHeight() - ($blockSport1.getHeight() + $blockSport2.getHeight() + $blockSportLink.getHeight());
                // set height blockSport2
                $blockSport2 = new Block($("#wrapper-sport>.wrapper:eq(1) .view-content"));
                $blockSport2.setHeight($blockSport2.getHeight()+$diff+$blockSportLink.getHeight());
            }
        }
    }
    /**
     * @return {[type]}
     */
    function resizeRubriqueMedium2() {
        if ($("#block-views-block-wrapper-rubrique-block-2").length) {
            // console.log("test function resizeRubriqueMedium2()");
            $wrapperBlockIntNat   = new Block($("#block-views-block-wrapper-rubrique-block-2"));
            $wrapperBlockInt      = new Block($("#wrapper-international"));
            $blockInt1            = new Block($("#wrapper-international>.wrapper:eq(0)"));
            $blockInt2            = new Block($("#wrapper-international>.wrapper:eq(1)"));
            $blockIntLink         = new Block($("#wrapper-international .view-footer"));
            $wrapperBlockNat      = new Block($("#wrapper-national"));
            $blockNat1            = new Block($("#wrapper-national>.wrapper:eq(0)"));
            $blockNat2            = new Block($("#wrapper-national>.wrapper:eq(1)"));
            $blockNatLink         = new Block($("#wrapper-national .view-footer"));
            $calcWrapper = $wrapperBlockInt.getHeight() - $wrapperBlockNat.getHeight();

            // console.log($calcWrapper);
            // console.log($blockIntLink.getHeight());
            if ($calcWrapper < 0) {
                // set height blockWrapperInt
                $wrapperBlockInt.setHeight($wrapperBlockInt.getHeight()+$calcWrapper*-1);
                $wrapperBlockInt      = new Block($("#wrapper-international"));
                $diff = $wrapperBlockInt.getHeight() - ($blockInt1.getHeight() + $blockInt2.getHeight() + $blockIntLink.getHeight());
                // set height blockInt2
                $blockInt2            = new Block($("#wrapper-international>.wrapper:eq(1) .view-content"));
                $blockInt2.setHeight($blockInt2.getHeight()+$diff+$blockIntLink.getHeight());

            }else{
                // set height blockWrapperNat
                $wrapperBlockNat.setHeight($wrapperBlockNat.getHeight()+$calcWrapper);
                $wrapperBlockNat      = new Block($("#wrapper-national"));
                $diff = $wrapperBlockNat.getHeight() - ($blockNat1.getHeight() + $blockNat2.getHeight() + $blockNatLink.getHeight());
                // set height blockNat2
                $blockNat2            = new Block($("#wrapper-national>.wrapper:eq(1) .view-content"));
                $blockNat2.setHeight($blockNat2.getHeight()+$diff+$blockNatLink.getHeight());

            }
        }
    }

    /**
     * modif margin-top block Edito
     */
    function resizeEdito() {

        $blockContent = new Block($('.path-frontpage #content'));
        $blockEdito = new Block($('#block-views-block-edito-block-1'));
        $blockEdito.setMarginTop("0px");
        $blockEdito = new Block($('#block-views-block-edito-block-1'));
        if (typeof($blockContent) !== undefined && typeof($blockContent) !== undefined ) {

            if ($blockContent.getBottom() > $blockEdito.getBottom()) {

                $calc = $blockContent.getBottom() - $blockEdito.getBottom();
                if ($calc < 0) { $calc = $calc*-1;}
                $blockEdito.setMarginTop(""+$calc+"px");
                $blockContent = new Block($('#content'));
                $blockEdito = new Block($('#block-views-block-edito-block-1'));
                if ($blockContent.getBottom() < $blockEdito.getBottom()){
                    $rapport=$blockEdito.getBottom() - $blockContent.getBottom();
                    if ($rapport < 0) { $rapport = $rapport*-1;}
                }
                if ($blockContent.getBottom() > $blockEdito.getBottom()){
                    $rapport= $blockContent.getBottom()-$blockEdito.getBottom();
                    if ($rapport < 0) { $rapport = $rapport*-1;}
                }
            }else{
                $rapport = 0;
            }
            if ($rapport < -1 || $rapport > 1) {
                setTimeout(resizeEdito, 500);
            }
        }
    }

    /** Class block
    *  @param {Object} block
    */
    function Block($block) {
        
        this.block = $block;
        // console.log(this.block);
        this.position = $block.offset();
        if (this.position != undefined) {
            this.top = this.position.top;
            this.left = this.position.left;
            this.bottom = $(window).height() - $block.height();
            this.bottom = this.position.top  - this.bottom;
            this.right = $(window).width() - $block.width();
            this.right = this.position.left - this.right;
        }else{
            this.position = 0;
        }
        this.element = document.getElementById($block.attr('id'));
        
        this.getBlock = function() {
            return this.block;
        }
        this.getElem = function() {
            return this.element;
        }
        if (this.position == 0) {

            this.getInfo = function() {
                return "block_position="+this.position;
            }
            this.getTop = function(){
                return this.top;
            }
            this.getBottom = function(){
                // alert(this.bottom);
                return this.bottom;
            }
            this.getLeft = function(){
                return this.left;
            }
            this.getRight = function(){
                return this.right;
            }
        }
        this.getWidth = function(){
            return this.block.width();
        }
        this.getHeight = function(){
            return this.block.height();
        }
        this.setHeight = function($height){
            return this.block.height($height);
        }
        this.setMarginTop = function($margin){
            return this.element.style.marginTop = $margin;
        }
        this.getMarginTop = function(){
            return this.element.style.marginTop;
        }
    }
    /**Class Block_sp
     * @param { Object }
     */
    function Block_sp ($block) {
        this.position = 0;
        this.element = document.getElementById($block.attr('id'));
        this.block = $block;

        this.getBlock = function() {
            return this.block;
        }
        this.getElem = function() {
            return this.element;
        }
        this.getWidth = function(){
            return this.block.width();
        }
        this.getHeight = function(){
            return this.block.height();
        }
        this.setHeight = function($height){
            return this.block.height($height);
        }
        this.setMarginTop = function($margin){
            return this.element.style.marginTop = $margin;
        }
    }

    /**
     * Hide open Menu
     */
    function hideMenu(){
        clearTimeout(hideTime); 
        btnMenuMagazines.attr("Class", "");
        btnMenuCommunes.attr("Class", "");
        btnMenuServices.attr("Class", "");
        menuMagazines.attr("Class", "hidden");
        menuCommunes.attr("Class", "hidden");
        menuServices.attr("Class", "hidden");
        menuMainElem.style.overflow = 'hidden';
    }
});
